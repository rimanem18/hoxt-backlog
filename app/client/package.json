{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port ${CLIENT_PORT:-3000}",
    "build": "next build",
    "build:static": "next build && next export",
    "start": "next start --port ${CLIENT_PORT:-3000}",
    "test": "bun test --dom --preload ./src/test-setup.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "check": "bun run typecheck && biome check src ../packages/shared-schemas",
    "fix": "biome check src ../packages/shared-schemas --write",
    "typecheck": "tsc --noEmit",
    "generate:types": "bunx openapi-typescript /home/bun/docs/api/openapi.yaml -o src/types/api/generated.ts"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-query": "^5.84.2",
    "next": "15.4.6",
    "openapi-fetch": "^0.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.2.22",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.10.1",
    "playwright": "^1.55.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
