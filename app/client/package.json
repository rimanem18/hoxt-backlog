{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port ${CLIENT_PORT:-3000}",
    "build": "next build",
    "build:static": "next build && next export",
    "start": "next start --port ${CLIENT_PORT:-3000}",
    "test": "bun test --dom --preload ./src/test-setup.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "check": "bun run typecheck && biome check src ../packages/shared-schemas",
    "fix": "biome check src ../packages/shared-schemas --write",
    "typecheck": "tsc --noEmit",
    "generate:types": "bunx openapi-typescript ../../docs/api/openapi.yaml -o src/types/api/generated.ts"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.0",
    "@supabase/supabase-js": "^2.86.2",
    "@tanstack/react-query": "^5.90.12",
    "next": "16.0.7",
    "openapi-fetch": "^0.15.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.2.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.3.3",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "jose": "^6.1.3",
    "jsdom": "^27.2.0",
    "openapi-typescript": "^7.10.1",
    "playwright": "^1.57.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  }
}
